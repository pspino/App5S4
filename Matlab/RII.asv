%H(z) 
% b = [1 -3.914 7.643 -9.551 8.717 -5.637 2.074];       %Coeff Nume
% a = [1 0.3696 0.04 ];                                 %Coeff Deno

function r = RII(s1,Fe)
    %1/H(z)
    b = [1 -3.914 7.643 -9.551 8.717 -5.637 2.074];         
    a = [1 0.3696 0.04];
    z = roots(a);
    p = roots(b);
    p = [.99*p(1); .99*p(2); .1*p(3); .5*p(4); .5*p(5); .5*p(6)];
    b = poly(z);
    a = poly(p);
    output = filter(b,a,s1);
    r = CoupeBandes(output,Fe);
end

